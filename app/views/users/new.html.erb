<div class=“container”>
 <h1>Sign Up!</h1>
 <%= form_with model: @user, local: true do |f| %>
   <div class=“form-group”>
     <%= f.label :email %>
     <%= f.text_field :email %>
   </div>
   <div class=“form-group”>
     <%= f.label :password %>
     <%= f.password_field :password %>
   </div>
   <%= f.submit %>
 <% end %>
</div>




